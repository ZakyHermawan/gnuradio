		<key>sd_spec0</key>
		<value></value>
		<type>string</type>
		<hide>
			#if not $num_mboards() > 0
				all
			#elif $sd_spec0()
				none
			#else
				part
			#end if
		</hide>
	</param>
	<param>
		<name>Mb1: Subdev Spec</name>
		<key>sd_spec1</key>
		<value></value>
		<type>string</type>
		<hide>
			#if not $num_mboards() > 1
				all
			#elif $sd_spec1()
				none
			#else
				part
			#end if
		</hide>
	</param>
	<param>
		<name>Mb2: Subdev Spec</name>
		<key>sd_spec2</key>
		<value></value>
		<type>string</type>
		<hide>
			#if not $num_mboards() > 2
